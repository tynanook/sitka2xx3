                                            > tset576    1            0         X         0        X       ; // LF 
                                            > tset576    1            0         X         0        X       ; // LF 
                                            > tset576    1            0         X         0        X       ; // LF msb x0A
repeat 99                                   
                                            > tset576xx  1            0         X         1        X       ; // Stop Bit =1
jump skip4                                  
                                            > -          -            -         -         -        -       ; 
set_loopA 65535                             
                                            > tset576xx  1            0         X         1        X       ; // Look for 'ok\r\n'                                            			                                             			
r_loop:
                                            > tset576xx  1            0         X         1        X       ; // look for Start Bit = L                                                								
if (fail) exit_loop go4, clr_fail, clr_cond 
                                            > tset576xx  1            0         X         1        H       ; //  branch on L                                              								
jump r_loop                                 
                                            > tset576xx  1            0         X         1        X       ; //  jump back on H                                              								
halt                                        
                                            > tset576xx  1            0         X         1        X       ; //  loopA time out	                                              								
go4:
ign, mask                                   
                                            > tset576xx  1            0         X         1        X       ; //  in Start Bit here                                              								
repeat 34, ign, mask                        
                                            > tset576xx  1            1         X         1        L       ; //  ~40% into Start Bit here                                              																							DUT is in TX mode, CW output at >+14dBm
                                            > tset576    1            0         X         1        H       ; // 'o' lsb 
                                            > tset576    1            0         X         1        H       ; // 'o'  
                                            > tset576    1            0         X         1        H       ; // 'o'  
                                            > tset576    1            0         X         1        H       ; // 'o'  
                                            > tset576    1            0         X         1        L       ; // 'o'  
                                            > tset576    1            0         X         1        H       ; // 'o'  
                                            > tset576    1            0         X         1        H       ; // 'o'  
                                            > tset576    1            0         X         1        L       ; // 'o' msb x6F 
                                            > tset576    1            0         X         1        H       ; // Stop Bit = 1
                                            > tset576    1            0         X         1        L       ; // Start Bit = 0
                                            > tset576    1            0         X         1        H       ; // 'k' lsb 
                                            > tset576    1            0         X         1        H       ; // 'k'  
                                            > tset576    1            0         X         1        L       ; // 'k'  
                                            > tset576    1            0         X         1        H       ; // 'k'  
                                            > tset576    1            0         X         1        L       ; // 'k'  
                                            > tset576    1            0         X         1        H       ; // 'k'  
                                            > tset576    1            0         X         1        H       ; // 'k'  
                                            > tset576    1            0         X         1        L       ; // 'k' msb x6B 
                                            > tset576    1            0         X         1        H       ; // Stop Bit = 1
                                            > tset576    1            0         X         1        L       ; // Start Bit = 0	
                                            > tset576    1            0         X         1        H       ; // CR lsb
                                            > tset576    1            0         X         1        L       ; // CR 
                                            > tset576    1            0         X         1        H       ; // CR 
                                            > tset576    1            0         X         1        H       ; // CR 
                                            > tset576    1            0         X         1        L       ; // CR 
                                            > tset576    1            0         X         1        L       ; // CR 
                                            > tset576    1            0         X         1        L       ; // CR 
                                            > tset576    1            0         X         1        L       ; // CR msb x0D
                                            > tset576    1            0         X         1        H       ; // Stop Bit = 1
                                            > tset576    1            0         X         1        L       ; // Start Bit = 0
                                            > tset576    1            0         X         1        L       ; // LF lsb
                                            > tset576    1            0         X         1        H       ; // LF 			
                                            > tset576    1            0         X         1        L       ; // LF 
                                            > tset576    1            0         X         1        H       ; // LF 
                                            > tset576    1            0         X         1        L       ; // LF 
                                            > tset576    1            0         X         1        L       ; // LF 
                                            > tset576    1            0         X         1        L       ; // LF 			
                                            > tset576    1            0         X         1        L       ; // LF msb x0A
skip4:
                                            > tset576    1            0         X         1        H       ; // Stop Bit = 1
repeat 4319                                 
                                            > tset576    1            0         X         1        X       ; // 75msec delay
repeat 100, ign, mask                       
                                            > tset576    1            0         X         1        X       ; //  clear pipeline														
halt                                        
                                            > tset576    1            0         X         1        X       ; 
}

